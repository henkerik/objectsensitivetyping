$condition = true; // 56
__log__ ("condition", $condition, 6);
$c = "NaN"; // 57 // "lookup"
if ($c){
    $name = "John"; // 59
    __log__ ("name", $name, 9);
}
else {
    $surname = "Doe"; // 60
    __log__ ("surname", $surname, 10);
}
