    $line = $line . $var; // 46
    $line = $line . $colon; // 47
    $line = $line . $type; // 48
    $line = $line . $newline; // 49
    file_put_contents ($file, $line, $flags);// , Call: 52, Return: 53, After: 50 function call
}
