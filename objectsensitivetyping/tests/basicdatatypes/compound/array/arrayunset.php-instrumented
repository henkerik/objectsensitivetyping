<?php
function __log__ ($var, $value, $label)  // "ln: 3, lx: 4, lxe: 5"
{
    $type = gettype ($value);// , Call: 7, Return: 8, After: 6 function call
    $cmp = "object"; // 10
    $is_object = $type == $cmp; // 11
    if ($is_object){
        $type = get_class ($value);// , Call: 14, Return: 15, After: 13 function call
    }
    else {
         // "17 skip"
    }
    $file = "log.txt"; // 18
    $newline = "\n"; // 19
    $flags = FILE_APPEND; // 20
    $colon = ":"; // 21
    $line = $label . $colon; // 22
    $line = $line . $var; // 23
    $line = $line . $colon; // 24
    $line = $line . $type; // 25
    $line = $line . $newline; // 26
    file_put_contents ($file, $line, $flags);// , Call: 28, Return: 29, After: 27 function call
}
